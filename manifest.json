{
    "manifest_version": 2,
    "name": "QA-components",
    "description": "Highlight app's components and provide brief description for QA",
    "version": "1.0.0",
    "background": {
        "scripts": [
            "index.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "For QA from Devs"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}